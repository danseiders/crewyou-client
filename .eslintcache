     lastName: profile.lastName,\n            imgURL: profile.imgURL,\n            airport: profile.airport,\n            about: profile.about,\n            position1: profile.position1,\n            position2: profile.position2,\n            position3: profile.position3,\n            position4: profile.position4,\n            touring: profile.touring,\n            availability: profile.availability\n        }, {withCredentials: true})\n        .catch(err => {\n            console.log(err)\n        }).then(res => {\n            console.log(res)\n            setRedirect(true)\n        })\n    }\n    const handleDelete = (event) => {\n        Axios.delete(`${REACT_APP_SERVER_URL}/profile/${profile.id}`)\n        .catch(err => {\n            console.log(err)\n        }).then(res => {\n            console.log(res)\n            props.setUser({profile: false})\n            setRedirect(true)\n\n        })\n    }\n\n    if(redirect){\n        return <Redirect to='/dashboard' />\n    }\n    return (\n        <div className='new-profile-container'>\n            <form className='new-profile-form-container' onSubmit={handleSubmit}>\n                <h1>Edit your profile</h1>\n                <label htmlFor='imgURL'>imgURL</label>\n                <input \n                    className='form-input'\n                    type='text' \n                    name='imgURL' \n                    id='imgURL' \n                    onChange={handleChange}\n                    defaultValue={profile.imgURL}>    \n                </input>\n                <label htmlFor='firstName'>First</label>\n                <input \n                    className='form-input'\n                    type='text' \n                    name='firstName' \n                    id='firstName' \n                    onChange={handleChange}\n                    defaultValue={profile.firstName}>    \n                </input>\n                <label htmlFor='lastName'>Last</label>\n                <input \n                    className='form-input'\n                    type='text' \n                    name='lastName' \n                    id='lastName' \n                    onChange={handleChange}\n                    defaultValue={profile.lastName}>\n                </input>\n                <label htmlFor='airport'>Airport</label>\n                <input \n                    className='form-input'\n                    type='text' \n                    name='airport' \n                    id='airport' \n                    onChange={handleChange}\n                    defaultValue={profile.airport}>\n                </input>\n                <label htmlFor='about'>Bio</label>\n                <input \n                    className='form-input'\n                    type='textarea' \n                    name='about' \n                    id='about' \n                    onChange={handleChange}\n                    defaultValue={profile.about}>    \n                </input>\n                <label htmlFor='position1'>1st Position</label>\n                <input \n                    className='form-input'\n                    type='text' \n                    name='position1' \n                    id='position1' \n                    onChange={handleChange}\n                    defaultValue={profile.position1}>   \n                </input>\n                <label htmlFor='position2'>2nd Position</label>\n                <input \n                    className='form-input'\n                    type='text' \n                    name='position2' \n                    id='position2' \n                    onChange={handleChange}\n                    defaultValue={profile.position2}>    \n                </input>\n                <label htmlFor='position3'>3rd Position</label>\n                <input \n                    className='form-input'\n                    type='text'\n                    name='position3' \n                    id='position3' \n                    onChange={handleChange}\n                    defaultValue={profile.position3}>    \n                </input>\n                <label htmlFor='position4'>4th Position</label>\n                <input \n                    className='form-input'\n                    type='text' \n                    name='position4' \n                    id='position4' \n                    onChange={handleChange}\n                    defaultValue={profile.position4}>    \n                </input>\n                <label htmlFor='touring'>Do you tour?</label>\n                <input \n                    className='form-input'\n                    type='radio' \n                    name='touring' \n                    id='touring' \n                    onChange={handleChange}\n                    defaultValue={profile.touring}>    \n                </input>\n                <label htmlFor='availability'>I'm available on</label>\n                <input \n                    className='form-input'\n                    type='text' \n                    name='availability' \n                    id='availability' \n                    onChange= {handleChange}\n                    defaultValue={profile.availability}>    \n                </input>\n                <input type='submit' value='Confirm Edits'></input>\n                <button onClick={handleDelete}>Delete Profile</button>\n            </form>\n        </div>\n    )\n }\n","/Users/DanSeiders/Documents/Coding/DS_Repos/crewyou/crewyou-client/src/components/Footer.js",[],"/Users/DanSeiders/Documents/Coding/DS_Repos/crewyou/crewyou-client/src/components/DashboardShow.js",[],"/Users/DanSeiders/Documents/Coding/DS_Repos/crewyou/crewyou-client/src/components/ProfileShow.js",[],"/Users/DanSeiders/Documents/Coding/DS_Repos/crewyou/crewyou-client/src/components/CrewList.js",[],"/Users/DanSeiders/Documents/Coding/DS_Repos/crewyou/crewyou-client/src/components/Messages.js",[],"/Users/DanSeiders/Documents/Coding/DS_Repos/crewyou/crewyou-client/src/components